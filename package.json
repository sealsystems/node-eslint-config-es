{
  "name": "@sealsystems/eslint-config-es",
  "version": "3.6.3",
  "description": "ESLint executable and configuration for SEAL Systems",
  "author": {
    "name": "SEAL Systems AG",
    "email": "opensource@sealsystems.de",
    "url": "https://www.sealsystems.de"
  },
  "license": "MIT",
  "bin": {
    "bot": "bin/bot",
    "lint": "bin/lint",
    "lint-fix": "bin/lint-fix"
  },
  "main": "lib/index.js",
  "scripts": {
    "bot": "node bin/bot",
    "licenses": "license-checker --production --csv",
    "lint:fix": "node bin/lint-fix",
    "lint": "node bin/lint",
    "release": "release",
    "test": "echo Not implemented.",
    "test:coverage:check": "echo Not implemented.",
    "test:coverage": "echo Not implemented.",
    "test:report": "echo Not implemented."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sealsystems/node-eslint-config-es.git"
  },
  "dependencies": {
    "depcheck": "0.9.2",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-mocha": "6.2.2",
    "eslint-plugin-prettier": "3.1.2",
    "prettier": "1.19.1",
    "shelljs": "0.8.3"
  },
  "devDependencies": {
    "@sealsystems/semantic-release": "4.0.18",
    "license-checker": "25.0.1"
  },
  "release": {
    "extends": "@sealsystems/semantic-release/node-module"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
